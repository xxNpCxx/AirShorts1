{
  "name": "airshorts-ai-video-generator",
  "version": "1.0.0",
  "description": "AI Video Generator Telegram Bot - создание персонализированных видео с ИИ-аватарами",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx tsc --build",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate": "ts-node src/migrate.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "nodemon --exec ts-node src/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@telegraf/types": "^7.1.0",
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "ioredis": "^5.3.2",
    "nestjs-telegraf": "^2.9.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "telegraf": "^4.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nestjs/cli": "^11.0.10",
    "@types/form-data": "^2.2.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "telegram",
    "bot",
    "nestjs",
    "typescript",
    "ai",
    "video-generator",
    "heygen",
    "d-id",
    "elevenlabs"
